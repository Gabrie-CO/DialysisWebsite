{
	"name": "mydialysiswebsite",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"docker:build": "node -e \"const fs=require('fs'); const env=fs.readFileSync('.env','utf8'); const match=env.match(/PUBLIC_CONVEX_URL=(.*)/); if(!match) { console.error('PUBLIC_CONVEX_URL not found in .env'); process.exit(1); } const url=match[1].trim(); require('child_process').spawn('docker', ['build', '--build-arg', 'PUBLIC_CONVEX_URL=' + url, '-t', 'dialysis-website', '.'], {stdio: 'inherit', shell: true});\"",
		"docker:run": "docker run --env-file .env -p 3000:3000 dialysis-website"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-node": "^5.5.2",
		"@sveltejs/adapter-vercel": "^6.3.3",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.1",
		"@types/node": "^25.2.3",
		"jsdom": "^28.0.0",
		"svelte": "^5.49.2",
		"svelte-check": "^4.3.6",
		"sveltekit-superforms": "^2.29.1",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"zod": "^3.25.76"
	},
	"dependencies": {
		"@sentry/sveltekit": "^10.38.0",
		"@tailwindcss/vite": "^4.1.18",
		"@workos-inc/node": "^8.5.0",
		"convex": "^1.32.0",
		"convex-svelte": "^0.0.12",
		"svelte-sonner": "^1.0.7",
		"tailwindcss": "^4.1.18",
		"valibot": "^1.2.0"
	}
}
