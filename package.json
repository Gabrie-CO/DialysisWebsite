{
	"name": "mydialysiswebsite",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"docker:build": "docker build --build-arg PUBLIC_CONVEX_URL=$(grep PUBLIC_CONVEX_URL .env | cut -d '=' -f2) -t dialysis-website .",
		"docker:run": "docker run --env-file .env -p 3000:3000 dialysis-website"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-node": "^5.5.2",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.1",
		"@types/node": "^25.2.3",
		"jsdom": "^28.0.0",
		"svelte": "^5.49.2",
		"svelte-check": "^4.3.6",
		"sveltekit-superforms": "^2.29.1",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"zod": "^3.25.76"
	},
	"dependencies": {
		"@sentry/sveltekit": "^10.38.0",
		"@tailwindcss/vite": "^4.1.18",
		"convex": "^1.31.7",
		"convex-svelte": "^0.0.12",
		"tailwindcss": "^4.1.18",
		"valibot": "^1.2.0"
	}
}
